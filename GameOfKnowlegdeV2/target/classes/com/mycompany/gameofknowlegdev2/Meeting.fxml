<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gameofknowlegdev2.MeetingController">
   <children>
      <ImageView fitHeight="275.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" />
      <HBox layoutY="275.0" prefHeight="125.0" prefWidth="600.0">
         <children>
            <TextArea fx:id="roomTextArea" prefHeight="125.0" prefWidth="300.0" />
            <TextArea fx:id="dialogTextArea" prefHeight="125.0" prefWidth="300.0" />
         </children>
      </HBox>
      <Button fx:id="endBtn" layoutX="534.0" layoutY="138.0" mnemonicParsing="false" onAction="#switchToEnd" text="Finish!" />
      <ProgressBar fx:id="credBar" layoutY="260.0" prefHeight="15.0" prefWidth="600.0" progress="0.32">
         <effect>
            <Glow level="0.75">
               <input>
                  <ColorAdjust hue="0.45" />
               </input>
            </Glow>
         </effect>
      </ProgressBar>
   </children>
</AnchorPane>
